<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="CalendarPickerView">
    <attr name="android:background"/>
    <attr name="tDividerColor" format="color"/>
    <attr name="tDayBackground" format="reference"/>
    <attr name="tDayTextColor" format="color"/>
    <attr name="tTitleTextColor" format="color"/>
    <attr name="tHeaderTextColor" format="color"/>
  </declare-styleable>

  <declare-styleable name="calendar_cell">
    <attr name="tState_selectable" format="boolean" />
    <attr name="tState_current_month" format="boolean" />
    <attr name="tState_today" format="boolean" />
    <attr name="tState_range_first" format="boolean" />
    <attr name="tState_range_middle" format="boolean" />
    <attr name="tState_range_last" format="boolean" />
    <attr name="tState_highlighted" format="boolean" />
  </declare-styleable>
</resources>
